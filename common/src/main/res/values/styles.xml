    <style name="IDefaultYearStyle">
        <item name="markerSize">24dp</item>
        <item name="marker">@drawable/ic_year_default_marker</item>
        <item name="beginLine">@color/grey_300</item>
        <item name="endLine">@color/grey_300</item>
        <item name="lineSize">2dp</item>
    </style>
</resources>

